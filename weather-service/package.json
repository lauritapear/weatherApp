{
  "name": "billing",
  "version": "1.0.0",
  "description": "billing export and request",
  "main": "./dist/server.js",
  "scripts": {
    "pretest": "tsc --skipLibCheck --pretty",
    "test": "npx nyc --reporter=lcov npx riteway -r ts-node/register src/test/**/**.test.ts",
    "integrationTest": "npx nyc --reporter=html ./node_modules/.bin/mocha  dist/testIntegration/*test.js ",
    "clean": "rimraf node_modules dist",
    "build": "tsc --skipLibCheck",
    "start": "tsc --skipLibCheck && node ./dist/server.js",
    "ts:watch": "tsc -w",
    "dev": "ts-node src/server.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node ./src/server.js",
    "lint": "eslint . --ext ts ",
    "lint:fix": "eslint . --ext ts --fix-dry-run",
    "spec": "mocha --require ts-node/register src/spec/*.spec.ts --reporter mochawesome --reporter-options reportDir=spec-report,reportFilename=spec",
    "spec:watch": "mocha --watch-extensions ts --watch --require ts-node/register src/spec/*.spec.ts"
  },
  "files": [
    "src"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^16.0.1",
    "@types/node": "^12.6.2",
    "@types/sinon-chai": "^3.2.2",
    "@types/uuid": "^3.4.5",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "2.8.5",
    "express": "^4.16.4",
    "express-error-handler": "^1.1.0",
    "express-fileupload": "^1.1.6",
    "express-winston": "^3.1.0",
    "helmet": "^3.21.1",
    "ini": "^1.3.5",
    "method-override": "^3.0.0",
    "multer": "^1.4.1",
    "number-format.js": "^2.0.8",
    "react": "^16.8.6",
    "request": "^2.88.0",
    "ts-node": "^8.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/express-winston": "^3.0.2",
    "@types/helmet": "0.0.43",
    "@types/jest": "^24.0.13",
    "@types/joi": "^14.3.3",
    "@types/lodash": "^4.14.134",
    "@types/method-override": "0.0.31",
    "@types/multer": "^1.3.7",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.1",
    "nodemon": "^1.19.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "riteway": "^6.0.3",
    "shelljs": "^0.8.3",
    "tap-nyc": "^1.0.3",
    "tape": "^4.10.2",
    "tslint": "^5.17.0",
